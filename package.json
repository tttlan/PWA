    "@angular/forms": "^7.0.2",
    "@angular/http": "^7.0.2",
    "@angular/material": "^7.0.2",
    "@angular/platform-browser": "^7.0.2",
    "@angular/platform-browser-dynamic": "^7.0.2",
    "@angular/platform-server": "^7.0.2",
    "@angular/pwa": "^0.10.4",
    "@angular/router": "^7.0.2",
    "@angular/service-worker": "^7.0.2",
    "@auth0/angular-jwt": "^2.0.0",
    "@ngrx/effects": "^6.1.2",
    "@ngrx/store": "^6.1.2",
    "@ngx-meta/core": "6.0.0",
