{
  "name": "RapidBox",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://tfs.bbv.ch/tfs/SAGCollection/_git/RapidBox"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200",
    "build": "ng build --prod --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract": "for lang in de en fr it; do ngx-translate-extract --input ./src --output ./src/assets/i18n/po/$lang.po --sort --format pot; done",
    "po2json": "for lang in de en fr it; do po2json src/assets/i18n/po/$lang.po src/assets/i18n/$lang.json --format mf; done",
    "translate": "npm run extract && npm run po2json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.2",
    "@angular/cdk": "^7.0.2",
